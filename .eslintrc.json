{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "rules": {
        "semi": 0,
        "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"] }],
        "comma-dangle": 0
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    }
}